<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">

	<beans profile="child">

		<!-- Controllers -->
		<bean id="fcfDocumentsMainController"
			class="org.fujionclinical.fhir.plugin.documents.stu3.DocumentMainController"
			scope="prototype" />

		<bean id="fcfDocumentsListController"
			class="org.fujionclinical.fhir.plugin.documents.stu3.DocumentListController"
			scope="prototype">
			<constructor-arg ref="fhirDocumentService" />
		</bean>

		<bean id="fcfDocumentsDisplayController"
			class="org.fujionclinical.fhir.plugin.documents.stu3.DocumentDisplayController"
			scope="prototype">
			<constructor-arg ref="fhirDocumentService" />
		</bean>

		<bean id="fcfDocumentsActionStatus" lazy-init="true"
			class="org.fujionclinical.fhir.plugin.documents.stu3.ActionStatus">
		</bean>

	</beans>
</beans>
