<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans 
		http://www.springframework.org/schema/beans/spring-beans.xsd">

	<beans profile="child">

		<bean parent="smartContextBase" depends-on="patientContext"
			  class="org.fujionclinical.fhir.smart.dstu2.SmartContextPatient" />

		<bean parent="smartContextBase" depends-on="encounterContext"
			  class="org.fujionclinical.fhir.smart.dstu2.SmartContextEncounter" />

	</beans>
</beans>
